{
  "lerna": "2.11.0",
  "nohoist": [
    "gatsby*",
    "babel*"
  ],
  "npmClient": "yarn",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "changelog": {
    "repo": "pfbreakaway/breakaway-patternfly-react",
    "cacheDir": ".changelog",
    "nextVersion": "Change Log",
    "labels": {
      "PR: breaking change :boom:": ":boom: Breaking Change",
      "PR: new feature :rocket:": ":rocket: New Feature",
      "PR: bug fix :bug:": ":bug: Bug Fix",
      "PR: css :nail_care:": ":nail_care: CSS",
      "PR: documentation :memo:": ":memo: Documentation",
      "PR: internal :house:": ":house: Internal"
    }
  },
  "command": {
    "publish": {
      "message": "chore(release): releasing packages",
      "conventionalCommits": true
    }
  },
  "version": "1.35.4",
  "useWorkspaces": true
}
